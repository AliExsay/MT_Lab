<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <TextView
            android:layout_width="62dp"
            android:layout_height="46dp" android:id="@+id/tv_carId"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="18dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp"/>
    <TextView
            android:layout_width="249dp"
            android:layout_height="47dp" android:id="@+id/tv_carName"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="17dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="78dp"/>
    <CheckBox
            android:layout_width="64dp"
            android:layout_height="47dp" android:id="@+id/cb_car"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="17dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="327dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>